<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.boyue.karaoke.model.LiveApk" table="liveapk" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="100" not-null="true">
            </column>
        </property>
        <property name="namee" type="java.lang.String">
            <column name="namee" length="100">
            </column>
        </property>
        <property name="name_jap" type="java.lang.String">
            <column name="name_jap" length="100">
            </column>
        </property>
        <property name="name_kor" type="java.lang.String">
            <column name="name_kor" length="100">
            </column>
        </property>
        <property name="path" type="java.lang.String">
            <column name="path" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="packName" type="java.lang.String">
            <column name="pack_name" not-null="true">
                <comment></comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
