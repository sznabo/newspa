<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.boyue.karaoke.model.HotelRegister" table="hotelregister"  >
        <id name="id" type="java.lang.Integer">
            <column name="id"  not-null="true">
                <comment>标识列</comment>
            </column>
        </id>
        <property name="hotelname" type="java.lang.String">
            <column name="hotelname" >
                <comment>酒店名称</comment>
            </column>
        </property>
        <property name="hotelid" type="java.lang.String">
            <column name="hotelid">
                <comment>酒店ID</comment>
            </column>
        </property>
         <property name="oldhotelname" type="java.lang.String">
            <column name="oldhotelname" >
                <comment>旧名称</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
