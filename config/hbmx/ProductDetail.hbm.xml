<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.boyue.karaoke.model.ProductDetail" table="product_detail"  >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="productId" type="java.lang.Integer">
            <column name="product_id" not-null="true">
                <comment>产品id</comment>
            </column>
        </property>
        <property name="moduleId" type="java.lang.Integer">
            <column name="module_id" not-null="true">
                <comment>模块id</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
