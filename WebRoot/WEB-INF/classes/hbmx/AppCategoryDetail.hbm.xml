<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.boyue.karaoke.model.AppCategoryDetail" table="app_category_detail" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="30" not-null="true">
                <comment>名称</comment>
            </column>
        </property>
        <property name="position" type="java.lang.Integer">
            <column name="position" not-null="true">
                <comment>位置</comment>
            </column>
        </property>
      	<property name="content" type="java.lang.String">
            <column name="content" not-null="true">
                <comment>内容</comment>
            </column>
        </property>
        <property name="contente" type="java.lang.String">
            <column name="contente" not-null="true">
                <comment>内容</comment>
            </column>
        </property>
        <property name="path" type="java.lang.String"> 
            <column name="path">
                <comment>图片位置 </comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
