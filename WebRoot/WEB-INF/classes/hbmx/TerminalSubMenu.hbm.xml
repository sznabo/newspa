<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.boyue.karaoke.model.TerminalSubMenu" table="terminal_submenu" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="255" not-null="true">
                <comment>名称</comment>
            </column>
        </property>
         <property name="namee" type="java.lang.String">
            <column name="namee" length="255" not-null="true">
                <comment>英文名称</comment>
            </column>
        </property>
         <property name="name_kor" type="java.lang.String">
            <column name="name_kor" length="255">
            </column>
        </property>
         <property name="name_jap" type="java.lang.String">
            <column name="name_jap" length="255">
            </column>
        </property>
        <property name="statu" type="java.lang.Integer">
            <column name="statu" not-null="true">
                <comment>开关状态</comment>
            </column>
        </property>
        <property name="menuId" type="java.lang.Integer">
            <column name="menu_id" not-null="true">
                <comment>主餐单id</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
