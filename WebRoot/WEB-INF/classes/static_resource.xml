 <bean id="nameList" class="java.util.ArrayList">
  <constructor-arg>
   <list>
    <value type="">liuyu</value>
   </list>
  </constructor-arg>
 </bean>
 -->
 
 <bean id="deviceTypes" class="java.util.TreeMap">
  <constructor-arg>
   <map>
    <entry key="1" value="分类1" />
    <entry key="2" value="分类 2" />
    <entry key="3" value="分类3" />
   </map>
  </constructor-arg>
 </bean>
 
 <bean id="deviceStatus" class="java.util.TreeMap">
  <constructor-arg>
   <map>
    <entry key="1" value="启用" />
    <entry key="2" value="停用" />
    <entry key="3" value="删除" />
   </map>
  </constructor-arg>
 </bean>
 
 <bean id="deviceRunStatus" class="java.util.TreeMap">
  <constructor-arg>
   <map>
    <entry key="1" value="良好" />
   </map>
  </constructor-arg>
 </bean>
 
 <!-- 消息 -->
  <bean id="messageStatus" class="java.util.TreeMap">
  <constructor-arg>
   <map>
    <entry key="0" value="停用" />
    <entry key="1" value="启用" />
   </map>
  </constructor-arg>
 </bean>
 
 <bean id="messagePosition" class="java.util.TreeMap">
  <constructor-arg>
   <map>
    <entry key="1" value="顶部" />
    <entry key="2" value="底部" />
   </map>
  </constructor-arg>
 </bean>
 
 <bean id="messagePlayType" class="java.util.TreeMap">
	<constructor-arg>
		<map>
			<entry key="1" value="实时播放"></entry>
			<entry key="2" value="定时播放"></entry>
		</map>
	</constructor-arg> 
</bean>
 
 <!-- 素材 -->
 <bean id="materialTypes" class="java.util.TreeMap">
  <constructor-arg>
   <map>
    <entry key="1" value="背景" />
    <entry key="2" value="音频" />
    <entry key="3" value="视频" />
    <entry key="4" value="图片" />
    <entry key="5" value="FLASH" />
    <entry key="6" value="PPT" />
    <entry key="7" value="word" />
    <entry key="8" value="excel" />
    <entry key="9" value="PDF" />
    <entry key="10" value="HTML" />
   </map>
  </constructor-arg>
 </bean>
 
 
 

</beans>